  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://github.com/plvo/next-utils",
  "bugs": {
    "url": "https://github.com/plvo/next-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plvo/next-utils"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "clean": "rimraf dist/",
    "build": "pnpm run clean && tsc",
